<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<includes>
	<include name="View_601_MusicBlox">
				<control type="group">
					<include>Animation.Common</include>
					<visible>Control.IsVisible(601)</visible>
					<top>110</top>
					<left>10</left>
					<width>1920</width>
					<bottom>10</bottom>
					<control type="image">
						<right>960</right>
						<top>0</top>
						<height>960</height>
						<texture fallback="E.png">$INFO[Container(601).Listitem(0).Icon]</texture>
				<aspectratio align="center" aligny="center">scale</aspectratio>
					</control>
			<control type="label">
						<height>260</height>
					<right>970</right>
					<width>260</width>
					<bottom>20</bottom>
				<font>symbolbiggerest</font>
				<label>$VAR[PercentWatchedBack]</label>
				<textcolor>white2</textcolor>
			</control>

			<control type="image">
						<height>260</height>
					<right>970</right>
					<width>260</width>
					<bottom>30</bottom>
				<texture>$VAR[ClearLogo]</texture>
				<aspectratio align="center" aligny="center">keep</aspectratio>
				<fadetime>50</fadetime>
			</control>

					</control>
		<control type="group">
			<include>Animation.Common</include>
			<visible>Control.IsVisible(601)</visible>
			<visible>!Window.IsVisible(DialogVideoInfo.xml) + !Window.IsVisible(DialogAlbumInfo.xml)</visible>
			<animation type="Conditional" reversible="true" condition="![Skin.HasSetting(osd.hidespoilers) + Container.Content(episodes) + [stringcompare(ListItem.Overlay,OverlayUnWatched.png) | ListItem.IsResumable]] + [Container.Content(musicvideos) | Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(sets) | Container.Content(episodes)] + !System.IdleTime(26) + System.IdleTime(5) + Skin.HasSetting(furniture.overlayinfo) + ![ControlGroup(9000).HasFocus() | Window.IsVisible(DialogContextMenu.xml)]">
				<effect type="fade" start="100" end="00" time="200" tween="quadratic" easing="out" />
				<effect type="zoom" end="1000,7" time="200" center="auto" tween="quadratic" easing="out" />
			</animation>
			<control type="panel" id="601">
				<top>110</top>
				<width>720</width>
				<left>970</left>
				<height>960</height>
				<onleft>60</onleft>
				<onright>60</onright>
				<onup>601</onup>
				<ondown>601</ondown>
				<preloaditems>2</preloaditems>
				<pagecontrol>60</pagecontrol>
				<viewtype label="31981">list</viewtype>
				<scrolltime tween="quadratic">400</scrolltime>
				<itemlayout height="240" width="240">
					<control type="image">
						<texture fallback="E.png">$INFO[Listitem.Icon]</texture>
						<aspectratio align="center" aligny="center">scale</aspectratio>
					</control>
				</itemlayout>
				<focusedlayout height="240" width="240">
					<control type="button" id="6020">
						<onfocus>RunScript(script.toolbox,info=blur,id='"$INFO[ListItem.Icon]"',radius=44,prefix=VideoInfo)</onfocus>
						<label>-</label>
						<focusedcolor>-</focusedcolor>
						<texturefocus>-</texturefocus>
						<texturenofocus>-</texturenofocus>
					</control>
					<control type="image">
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="110" delay="0" time="50" tween="bounce" easing="out" reversible="false" center="auto" />
					</animation>
						<texture fallback="E.png">$INFO[Listitem.Icon]</texture>
				<aspectratio align="center" aligny="center">scale</aspectratio>
					</control>
				</focusedlayout>
			</control>
			<include>Furniture_ScrollbarBannerPlex</include>
		</control>

	</include>
</includes>